</head>

<body>
  <header>
    <nav>
      <div>
        <h2>ADMIN AUTH</h2>
        <a href="/" class="home"><i class="material-icons">home</i></a>
      </div>
    </nav>
  </header>
  <section>

    </div>
    <div class="form-body z-depth-2">
      <div class="row">
        <div class="col s12 m12 l12">
          <ul class="tabs">
            <li class="tab col s3 m6 ">
              <a href="#login" class="active">login</a></li>
          </ul>
        </div>

        <div id="login" class="col s12">
          <form action='/admin/auth/login' method="POST" autocomplete="off">

            <div class="input wrapper input-field">
              <i class="material-icons prefix">phone</i>
              <label for="phone">Phone</label>
              <input id="phone" name="phone" type="number"> </div>
            <div class="input wrapper input-field">
              <i class="material-icons prefix">lock</i>
              <label for="pwd">password</label>
              <input id="pwd" name="pwd" type="password"> </div>
            <div class="button wrap"><button type="submit">Submit</button></div>
          </form>
        </div>
      </div>
    </div>
  </section>
  <script src="/js/materialize.min.js"></script>
  <script src="/newChannelForm.js"></script>
</body>

</html>